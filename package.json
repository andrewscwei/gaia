{
  "name": "@andrewscwei/gaia",
  "version": "0.22.0",
  "description": "Geospatial utility functions for Node.js, powered by Turf.js",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run lint",
    "build": "npm run clean && tsc",
    "test": "npm run build -- --project tsconfig.test.json && mocha --config .mocharc --extension spec.js \"build/$npm_config_files\"",
    "test:ts": "cross-env DEBUG=gaia* mocha --config .mocharc -n loader=ts-node/esm -r ts-node/register --extension spec.ts \"src/$npm_config_files\"",
    "lint": "eslint --ext .ts src",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewscwei/gaia.git"
  },
  "files": [
    "build"
  ],
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewscwei/gaia/issues"
  },
  "homepage": "https://github.com/andrewscwei/gaia#readme",
  "types": "build/index.d.ts",
  "devDependencies": {
    "@turf/turf": "^6.5.0",
    "@types/chai": "^4.3.4",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "mocha": "^10.2.0",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@turf/nearest-point-to-line": "^6.5.0",
    "@turf/point-to-line-distance": "^6.5.0",
    "debug": "^4.3.4",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "@turf/turf": "^6.5.0"
  }
}
